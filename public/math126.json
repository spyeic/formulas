[
    {
        "title": "Vector Formulas",
        "description": "`\\textbf{a} = <a_1, a_2, a_3>`, `\\textbf{b} = <b_1, b_2, b_3>`, `\\textbf{m} = <m_1, m_2>`, `\\textbf{n} = <n_1, n_2>`",
        "formulas": [
            {
                "title": "Dot Product",
                "formulas": [
                    "\\textbf{a} \\cdot \\textbf{b} = a_1b_1 + a_2b_2 + a_3b_3",
                    "\\textbf{a} \\cdot \\textbf{b} = |\\textbf{a}||\\textbf{b}|\\cos(\\theta)",
                    "\\textbf{a} \\cdot \\textbf{a} = |\\textbf{a}|^2"
                ]
            },
            {
                "title": "Cross Product",
                "formulas": [
                    "\\begin{bmatrix}a & b\\\\c & d\\end{bmatrix} = ad - bc",
                    "\\begin{bmatrix}a & b & c\\\\d & e & f\\\\g & h & i\\end{bmatrix} = a\\begin{bmatrix}e & f\\\\h & i\\end{bmatrix} - b\\begin{bmatrix}d & f\\\\g & i\\end{bmatrix} + c\\begin{bmatrix}d & e\\\\g & h\\end{bmatrix}",
                    "\\textbf{a} \\times \\textbf{b} = \\begin{bmatrix}i & j & k\\\\a_1 & a_2 & a_3\\\\b_1 & b_2 & b_3\\end{bmatrix} = i(a_2b_3 - a_3b_2) - j(a_1b_3 - a_3b_1) + k(a_1b_2 - a_2b_1)",
                    "|\\textbf{a} \\times \\textbf{b}| = |\\textbf{a}||\\textbf{b}|\\sin(\\theta)",
                    "\\textbf{a} \\times \\textbf{b} = 0 \\text{ if } \\textbf{a} \\parallel \\textbf{b}"
                ]
            },
            "\\text{comp}_{\\textbf{a}}\\textbf{b} = \\frac{\\textbf{a} \\cdot \\textbf{b}}{|\\textbf{a}|}",
            "\\text{proj}_{\\textbf{a}}\\textbf{b} = \\frac{\\textbf{a} \\cdot \\textbf{b}}{\\textbf{a} \\cdot \\textbf{a}}\\textbf{a}"
        ]
    },
    {
        "title": "Vector Equation of a Line",
        "description": "`\\textbf{r}_0 = <x_0, y_0, z_0>` is the initial position textbftor and `t` is the parameter",
        "formulas": [
            "\\textbf{r} = \\textbf{r}_0 + t\\textbf{v} = <x_0, y_0, z_0> + t<v_1, v_2, v_3>",
            {
                "title": "Parametric Equations",
                "formulas": [
                    "\\textbf{r} = <x_0 + at, y_0 + bt, z_0 + ct>",
                    "x = x_0 + tv_1",
                    "y = y_0 + tv_2",
                    "z = z_0 + tv_3"
                ]
            },
            {
                "title": "Symmetric Equations",
                "formulas": [
                    "\\frac{x - x_0}{a} = \\frac{y - y_0}{b} = \\frac{z - z_0}{c}"
                ]
            }
        ]
    },
    {
        "title": "Vector Equation of a Plane",
        "description": "`\\textbf{r}_0 = <x_0, y_0, z_0>` is a point on the plane and `\\textbf{n} = <a, b, c>` is the normal textbftor",
        "formulas": [
            "\\textbf{n} \\cdot (\\textbf{r} - \\textbf{r}_0) = 0",
            "a(x - x_0) + b(y - y_0) + c(z - z_0) = 0",
            "ax + by + cz + d = 0",
            {
                "title": "Distance from a Point to a Plane",
                "formulas": [
                    "d = \\frac{|ax_0 + by_0 + cz_0 + d|}{\\sqrt{a^2 + b^2 + c^2}}"
                ]
            }
        ]
    },
    {
        "title": "Derivatives and Integrals of Vector Functions",
        "description": "`\\textbf{r}(t) = <f(t), g(t), h(t)>`",
        "formulas": [
            "\\textbf{r}'(t) = <f'(t), g'(t), h'(t)>",
            "\\int_b^a\\textbf{r}(t)dt = <\\int_b^a f(t)dt, \\int_b^a g(t)dt, \\int_b^a h(t)dt>",
            {
                "title": "Differentiation Rules",
                "formulas": [
                    "[\\textbf{u}(t) + \\textbf{v}(t)]' = \\textbf{u}'(t) + \\textbf{v}'(t)",
                    "[c\\textbf{u}(t)]' = c\\textbf{u}'(t)",
                    "[f(t)\\textbf{u}(t)]' = f'(t)\\textbf{u}(t) + f(t)\\textbf{u}'(t)",
                    "[\\textbf{u}(t) \\cdot \\textbf{v}(t)]' = \\textbf{u}'(t) \\cdot \\textbf{v}(t) + \\textbf{u}(t) \\cdot \\textbf{v}'(t)",
                    "[\\textbf{u}(t) \\times \\textbf{v}(t)]' = \\textbf{u}'(t) \\times \\textbf{v}(t) + \\textbf{u}(t) \\times \\textbf{v}'(t)",
                    "[\\textbf{u}(f(t))] = \\textbf{u}'(f(t))f'(t)"
                ]
            },
            "\\text{If }|r(t)| = c \\text{ then } \\textbf{r}(t) \\cdot \\textbf{r}'(t) = 0"
        ]
    },
    {
        "title": "Arc Length and Curvature",
        "description": "`\\textbf{r}(t) = <f(t), g(t), h(t)>`",
        "formulas": [
            "\\textbf{T}(t) = \\frac{\\textbf{r}'(t)}{|\\textbf{r}'(t)|}",
            "\\textbf{N}(t) = \\frac{\\textbf{T}'(t)}{|\\textbf{T}'(t)|}",
            "\\textbf{B}(t) = \\textbf{T}(t) \\times \\textbf{N}(t)",
            {
                "title": "Arc Length",
                "formulas": [
                    "s = \\int_a^b|\\textbf{r}'(t)|dt",
                    "\\frac{ds}{dt} = |\\textbf{r}'(t)|",
                    "s = \\int_a^b\\sqrt{[f'(t)]^2 + [g'(t)]^2 + [h'(t)]^2}dt"
                ]
            },
            {
                "title": "Curvature",
                "formulas": [
                    "\\kappa = |\\frac{d\\textbf{T}}{ds}|",
                    "\\kappa(t) = \\frac{|\\textbf{T}'(t)|}{|\\textbf{r}'(t)|} = \\frac{|\\textbf{r}'(t) \\times \\textbf{r}''(t)|}{|\\textbf{r}'(t)|^3}",
                    "\\kappa(x) = \\frac{|f''(x)|}{[1 + [f'(x)]^2]^{3/2}} \\text{ if } y = f(x)"
                ]
            }
        ]
    }
